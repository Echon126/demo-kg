<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.TParamGroupMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.domain.entity.TParamGroup">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="device_type_key" property="deviceTypeKey" />
        <result column="owner_account" property="ownerAccount" />
        <result column="group_sort" property="groupSort" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, device_type_key, owner_account, group_sort
    </sql>

</mapper>
